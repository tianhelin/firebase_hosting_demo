{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/test",
        "destination": "/huicheng_shop/index.html"
      },
      {
        "source": "/test/.*",
        "destination": "/huicheng_shop/$1"
      }
    ]
  }
}
